<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_post">

    <fragment
        android:id="@+id/navigation_sample_app_list"
        android:name="com.haya.android_no_mori.ui.post.PostFragment"
        android:label="@string/title_sample_app"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_post"
        android:name="com.haya.android_no_mori.ui.sampleapplist.SampleAppListFragment"
        android:label="@string/title_post"
        tools:layout="@layout/fragment_sample_app_list" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.haya.android_no_mori.ui.setting.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/fragment_notifications" />
</navigation>